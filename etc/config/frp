
config frp 'common'
	option log_max_days '3'
	option login_fail_exit '0'
	option enable_cpool '0'
	option time '40'
	option tcp_mux '1'
	option log_level 'info'
	option enable_http_proxy '0'
	option protocol 'tcp'
	option enabled '1'
	option server_addr '027028.xyz'
	option server_port '2000'
	option token '888888'
	option tls_enable '0'
	option admin_enable '0'
	option vhost_http_port '9999'
	option vhost_https_port '9998'

config proxy
	option type 'tcp'
	option remote_port '6881'
	option local_ip '192.168.1.2'
	option local_port '6881'
	option proxy_protocol_version 'disable'
	option use_encryption '1'
	option use_compression '1'
	option remark 'TCP6881'
	option enable '1'

